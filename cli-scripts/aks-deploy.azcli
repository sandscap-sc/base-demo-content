az group create --location "eastus" --name "base-demo-aks-rg"

az aks get-versions --location "eastus" -o table

az vm list-sizes --location "eastus" -o table

az aks create --name "base-demo-aks" --resource-group "base-demo-aks-rg" --dns-name-prefix "basedemoaks" --kubernetes-version 1.10.8 --node-count 3 --node-vm-size "Standard_D2s_v3"

az aks get-credentials -n base-demo-aks -g base-demo-aks-rg
